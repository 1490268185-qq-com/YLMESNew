
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>RepairOrderDrawing</title>
    <script src="~/js/vue.js"></script>
    <script src="~/js/axios.js"></script>
    <script src="~/js/elemenui.js"></script>
    <link href="~/css/elemenui.css" rel="stylesheet" />
    <script type="text/javascript">
        "use script"

        window.onload = function () {
            var vm = new Vue({
                el: '#vues',
                data: {
                    tableData: []
                }
            })
        }
    </script>
</head>
<body>
    <div id="vues">
        <template>
            <el-breadcrumb separator-class="el-icon-arrow-right" :style=sty>
                <el-breadcrumb-item :to="{ path: '/' }">首页</el-breadcrumb-item>
                <el-breadcrumb-item>车间生产管理</el-breadcrumb-item>
                <el-breadcrumb-item>工单图纸</el-breadcrumb-item>
            </el-breadcrumb>
            <el-table style="margin:0 auto;box-shadow: 0 2px 4px rgba(0, 0, 0, .12), 0 0 6px rgba(0, 0, 0, .04)"

                      element-loading-background="rgba(0, 0, 0, 0.8)"
                      :data="tableData"
                      @@row-click="rowclick">


            </el-table>
        </template>
    </div>
</body>
</html>
