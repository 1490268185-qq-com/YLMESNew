
@{
    Layout = null;
}

<!DOCTYPE HTML PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
<head>

    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <title>友力信息管理系统</title>
    <link href="~/css/style.css" rel="stylesheet" type="text/css" />
    <link href="~/layui/css/layui.css" rel="stylesheet" />
    <script src="~/My97DatePicker4.7.2/WdatePicker.js"></script>
    <script src="~/layui/layui.js" type="text/javascript"></script>
    <script type="text/javascript" src="~/js/jquery-1.9.1.min.js"></script>
    <script type="text/javascript" src="~/lib/layer/2.4/layer.js"></script>
    <script type="text/javascript" src="~/lib/laypage/1.2/laypage.js"></script>
    <script type="text/javascript" src="~/js/Laimingsh.js"></script>
    <script type="text/javascript">
    </script>
    <script type="text/html" id="barDemo">
        <a class="layui-btn layui-btn-xs" lay-event="upload">附件</a>
        <a class="layui-btn layui-btn-xs" lay-event="detail">反审</a>
        @*<a class="layui-btn layui-btn-danger layui-btn-xs" lay-event="del">删除</a>*@
    </script>
</head>
<body>
    <img src="" style="display:none;width:100%;height:100%" class="imgg" />

    @*新增上传附件*@
    <div class="layui-form czs" style="display:none" action="" id="for3">
        <fieldset class="layui-elem-field layui-field-title " style="margin-top: 20px;color:mediumseagreen;">
            <legend>报销单原始凭证上传</legend>
        </fieldset>
        <div class="layui-form-item">
            <blockquote class="layui-elem-quote layui-quote-nm" style="margin-top: 10px;">
                原附件预览图：
                <div class="layui-upload-list" id="demo3"></div>
            </blockquote>

        </div>
        <div class="layui-form-item">
            <div class="layui-input-block">
                <button class="layui-btn" id="qx3">取消</button>
                <input type="text" id="flag4" hidden="hidden" value="False" />
            </div>
        </div>

    </div>

    <div class="place">
        <select hidden="hidden" name="modules" lay-search=lay-search id="gssname" lay-verify="gssname" lay-filter='gssname'></select>
        <span>位置：</span><input type="text" id="Sessioname" hidden="hidden" value=@Session["name"] />
        <ul class="placeul">
            <li><a href="#">首页</a></li>
            <li><a href="#">财务管理</a></li>
            <li><a href="#">费用报销单</a></li>
        </ul>
    </div>
    <div class="rightinfo">
        <div class="tools">
            <ul class="toolbar">

                <li>
                    时间从：
                    <input name="dates" class="strattime" id="dates" type="text" onclick="WdatePicker()" style="width: 80px;" />
                </li>
                <li>
                    到：
                    <input name="datee" class="endtime" id="datee" type="text" onclick="WdatePicker()" style="width: 80px;" />
                </li>
                <li>
                    状态：
                    <select name="modules" lay-search=lay-search id="gssz" lay-verify="gssz" lay-filter="gssz">
                        <option value="">请选择</option>
                        <option value="">全部</option>
                        <option value="已">已审核</option>
                        <option value="待">待审核</option>
                    </select>
                </li>
            </ul>

            <div id="divsh">  <button class="layui-btn" id="Search" data-type="reload">搜索</button><button class="layui-btn" id="shenhe" data-type="reload">审核</button></div>
        </div>
        <table class="tablelist" id="tw" lay-filter="demo"></table>

    </div>
    @*单号查看*@
    <div class="layui-form czs" style="display:none" action="" id="for5">
        <div class="layui-form-item">
            <table class="tablelist" id="tw9" lay-filter="demo"></table>
        </div>
    </div>
    <script type="text/html" id="cc">
        {{#  if(d.单号 != '' && d.单号!=null){ }}
        <span style="color:red;cursor:pointer;">{{d.单号}}</span>
        {{#  } else { }}
        {{#  } }}
    </script>
    <script type="text/javascript">
        $(function () {
            if ('@Session["name"]' == "") {
                location.href = '/Home/Login'
            }
        });
    </script>
</body>
</html>
