
@{
    Layout = null;


}

<!DOCTYPE html>
<html xmlns="http://www.w3.org/1999/xhtml">
<head>

    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <title>友力信息管理系统</title>
    <script src="../../My97DatePicker4.7.2/WdatePicker.js"></script>
    <link href="~/css/style.css" rel="stylesheet" type="text/css" />
    <link href="~/layui/css/layui.css" rel="stylesheet" />
    <script src="~/layui/layui.js" type="text/javascript"></script>
    <script type="text/javascript" src="~/js/jquery-1.9.1.min.js"></script>
    <script type="text/javascript" src="~/lib/layer/2.4/layer.js"></script>
    <script type="text/javascript" src="~/lib/laypage/1.2/laypage.js"></script>


</head>


<body onselectstart="return false;">
    <div class="place">
        <span>位置：</span>
        <ul class="placeul">
            <li><a href="#">首页</a></li>
            <li><a href="#">成品仓</a></li>
            <li><a href="#">产品包装</a></li>
        </ul>
    </div>
    <div class="rightinfo">
        <table class="tablelist" id="tw" lay-filter="demo"></table>
    </div>
    <script type="text/javascript" src="~/lib/jquery/1.9.1/jquery.min.js"></script>
    <script type="text/javascript" src="~/lib/layer/2.4/layer.js"></script>
    <script type="text/javascript" src="~/static/h-ui/js/H-ui.min.js"></script>
    <script type="text/javascript" src="~/static/h-ui.admin/js/H-ui.admin.js"></script>
    <script type="text/javascript" src="~/lib/laypage/1.2/laypage.js"></script>
    <script type="text/javascript">
        layui.use(['table', 'layer'], function () {
            var table = layui.table, layer = layui.layer;
            var tableIns = table.render({
                elem: '#tw'
                   , url: '/FinishProductHouse/CheckFinishedProductWarehouse?id='+'@ViewData["id"]'
                   , cols: [[
                    { field: '', hide: true }
                   //,{ field: 'ID', width: 100, title: '序号' }
                   , { field: 'Reservoir', width: 90, title: '库区' }
                   , { field: 'CargoArea', width: 90, title: '货区' }
                   , { field: 'Goods', width: 90, title: '货位' }
                   , { field: 'PartNumber', width: 250, title: '产品名称' }
                   , { field: 'PartSpec', width: 180, title: '产品规格' }
                   , { field: 'QTY', width: 180, title: '数量' }
                   , { field: 'PackageName', width: 180, title: '包装条码' }
                   , { field: 'ContractNumber', width: 180, title: '项目编号' }
                   , { field: 'right', width: 350, align: 'center', toolbar: '#barDemo' }
                   ]]

                });             
        });
    </script>
    <script type="text/javascript">
        $('.tablelist tbody tr:odd').addClass('odd');
    </script>
</body>

</html>
